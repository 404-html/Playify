{
 "remoteMethods" :  [
	{
	"name":"verifyLoginInformation",
     "object":"LoginDispatcher",
     "call-semantics":"at-most-one",
     "param":
          {
              "user":"string",
              "password":"string"
          },
        "return":"String"
     },
     
     {
     	"name": "verifyRegisterInformation",
     	"object": "RegisterDispatcher",
     	"call-semantics": "at-most-one",
     	"param":
     		{
     			"user":"string",
     			"password":"string",
     			"confirmPassword":"string"
     		},
     	"return":"String"
     },
     
     
     {
	"name":"removeSongFromPlaylist",
     "object":"PlaylistDispatcher",
     "call-semantics":"at-most-one",
     "param":
          {
              "user":"string",
              "playlist":"string",
              "songID" : "string"
          },
        "return":"String"
     },
     
     {
	"name":"removePlaylist",
     "object":"PlaylistDispatcher",
     "call-semantics":"at-most-one",
     "param":
          {
              "user":"string",
              "playlist":"string"
          },
        "return":"String"
     },
     
     
     {
	"name":"createAndAddPlaylist",
     "object":"PlaylistDispatcher",
     "call-semantics":"at-most-one",
     "param":
          {
              "user":"string",
              "playlist":"string"
          },
        "return":"String"
     },
     
     
     {
        "name":"getSongChunk",
        "object":"SongDispatcher",
        "call-semantics":"maybe",
        "param":
          {
              "song":"Long",
              "fragment":"Long"
          },
        "return":"Byte[]"
   	},
   	
   	{
   	"name":"searchForSongs",
     "object":"SongDispatcher",
     "call-semantics":"at-most-one",
     "param":
          {
              "searchInput":"string"
          },
        "return":"String"
     }
   	

	]
}